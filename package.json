{
  "name": "repodoc",
  "version": "0.0.17",
  "description": "creates a overview site for a monorepo",
  "main": "repodoc.js",
  "bin": {
    "repodoc": "./bin/repodoc.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jmo3300/repodoc"
  },
  "scripts": {
    "prebuild": "rimraf ./bin",
    "build": "tsc",
    "doc": "typedoc -out ./docs/html --exclude \"**/*+(index|.spec|.e2e).ts\" && cpx ./docs/assets/**/* ./docs/html/docs/assets",
    "test": "mocha -r ts-node/register src/**/config.spec.ts"
  },
  "keywords": [
    "repodoc",
    "compodoc",
    "angular",
    "documentation generator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@octokit/auth-basic": "^1.4.5",
    "@octokit/rest": "^18.0.0",
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^6.5.0",
    "@types/rimraf": "^3.0.0",
    "@types/yargs": "^15.0.5",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "configstore": "^5.0.1",
    "figlet": "^1.4.0",
    "fs-extra": "^9.0.1",
    "handlebars": "^4.7.6",
    "inquirer": "^7.2.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.5",
    "simple-git": "^3.5.0",
    "touch": "^3.1.0",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.5",
    "valid-filename": "^3.1.0",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cpx": "^1.5.0",
    "mocha": "^8.0.1",
    "ts-node": "^8.10.2"
  }
}
